<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/color_bg"
    tools:context=".Faculty">

    <RelativeLayout
        android:layout_width="405dp"
        android:layout_height="592dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout"
        app:layout_constraintVertical_bias="0.495">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="157dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:layout_weight="1"
            android:gravity="center">

            <androidx.cardview.widget.CardView
                android:id="@+id/c166"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_marginLeft="55dp"
                android:layout_weight="1"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="160dp"
                    android:layout_height="83dp"
                    android:layout_gravity="center"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/books" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="159dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="122dp"
                    android:gravity="center"
                    android:text="Books"
                    android:textColor="@color/black"
                    android:textStyle="bold|italic" />
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/c167"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="183dp"
                    android:layout_height="99dp"
                    android:layout_gravity="center"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/renew" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="159dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="122dp"
                    android:gravity="center"
                    android:text="Renew Book"
                    android:textColor="@color/black"
                    android:textStyle="bold|italic" />
            </androidx.cardview.widget.CardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="376dp"
            android:layout_height="163dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="29dp"
            android:layout_marginBottom="255dp"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:id="@+id/c168"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">

                <ImageView

                    android:layout_width="150dp"
                    android:layout_height="91dp"
                    android:layout_gravity="center"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/j1" />

                <TextView
                    android:id="@+id/textView47"
                    android:layout_width="159dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="118dp"
                    android:gravity="center"
                    android:text="My Books"
                    android:textColor="@color/black"
                    android:textStyle="bold|italic" />
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/c169"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">

                <ImageView

                    android:layout_width="268dp"
                    android:layout_height="134dp"
                    android:layout_gravity="center"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/cash" />

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="162dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="118dp"
                    android:gravity="center"
                    android:text="Fines"
                    android:textColor="@color/black"
                    android:textStyle="bold|italic" />
            </androidx.cardview.widget.CardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="376dp"
            android:layout_height="168dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="29dp"
            android:layout_marginBottom="57dp"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:id="@+id/c170"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">

                <ImageView
                    android:layout_width="198dp"
                    android:layout_height="109dp"
                    android:layout_gravity="center"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/pre" />

                <TextView
                    android:id="@+id/textView227"
                    android:layout_width="159dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="120dp"
                    android:gravity="center"
                    android:text="Premium Content"
                    android:textColor="@color/black"
                    android:textStyle="bold|italic" />
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/c171"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">

                <ImageView
                    android:layout_width="290dp"
                    android:layout_height="131dp"
                    android:layout_gravity="center"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/suggestions" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="159dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="118dp"
                    android:gravity="center"
                    android:text="Suggestions"
                    android:textColor="@color/black"
                    android:textStyle="bold|italic" />
            </androidx.cardview.widget.CardView>
        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="114dp"
            android:layout_marginBottom="31dp"
            android:text="Welcome back!"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold|italic" />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>